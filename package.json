{
  "name": "app-platform-ui",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "npm run build:commit; ng serve --aot --host 0.0.0.0 --port 8081",
    "build": "ng build --aot --prod --prod",
    "build:serversdk": "cd ../app-platform-server; ./node_modules/.bin/lb-sdk server/server ../app-platform-ui/src/modules/platform-server-sdk/lib/serversdk -d ng2web -i disabled -v enabled",
    "build:commit": "git add -A .; git commit -am'build:commit'; git push",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "5.0.1",
    "@angular/compiler": "5.0.1",
    "@angular/core": "5.0.1",
    "@angular/forms": "5.0.1",
    "@angular/http": "5.0.1",
    "@angular/platform-browser": "5.0.1",
    "@angular/platform-browser-dynamic": "5.0.1",
    "@angular/router": "5.0.1",
    "@ngtools/webpack": "1.8.0",
    "core-js": "^2.4.1",
    "immutable": "^3.8.1",
    "jquery": "^3.1.1",
    "ng-semantic": "https://github.com/vladotesanovic/ngSemantic/tarball/master",
    "rxjs": "^5.0.1",
    "semantic-ui": "^2.2.7",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-beta.30",
    "@angular/compiler-cli": "5.0.1",
    "@types/jasmine": "2.6.3",
    "@types/node": "^8.0.51",
    "codelyzer": "~4.0.1",
    "gulp": "^3.9.1",
    "jasmine-core": "2.9.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.6.0",
    "protractor": "~5.3.0",
    "ts-node": "3.3.0",
    "tslint": "^5.8.0",
    "typescript": "~2.6.1",
    "webdriver-manager": "12.0.6",
    "webpack": "3.8.1"
  }
}
